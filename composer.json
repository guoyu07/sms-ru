{
    "name": "romach3/sms-ru",
    "description": "Sms.ru Notifications driver.",
    "keywords": ["laravel", "notifications", "sms.ru"],
    "homepage": "https://github.com/romach3/sms-ru",
    "license": "MIT",
    "authors": [
        {
            "name": "romach3",
            "role": "Developer",
            "email": "romach3@gmail.com"
        },
        {
            "name": "JhaoDa",
            "role": "Developer",
            "email": "jhaoda@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/queue": "^5.3@dev",
        "illuminate/events": "^5.3@dev",
        "illuminate/support": "^5.3@dev",
        "illuminate/notifications": "^5.3@dev"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.8|~5",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\SmsRu\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\SmsRu\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev"
}
